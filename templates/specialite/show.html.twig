{% extends 'base.html.twig' %}

{% block title %}Specialite
{% endblock %}

{% block body %}
	<div class="container-fluid dashboard-div">
		<div class="row top-actions">
			<div class="col-8 top-header">
				<h4>Specialite</h4>
			</div>

		</div>
		<table class="table">
			<tbody>

				<tr>
					<th>Nom</th>
					<td>{{ specialite.nom }}</td>
				</tr>
				<tr>
					<th>Description</th>
					<td>{{ specialite.description }}</td>
				</tr>
			</tbody>
		</table>

        <a class="btn btn-danger" href="{{ path('specialite_index') }}"><i class="fas fa-chevron-circle-left"></i></a>
        <div style="float: right;" class="col-2 actions text-right">
            <a class="btn btn-warning" href="{{ path('specialite_edit', {'id': specialite.id}) }}"><i
                        class="fas fa-edit"></i></a>
        </div>

	</div>

{% endblock %}
